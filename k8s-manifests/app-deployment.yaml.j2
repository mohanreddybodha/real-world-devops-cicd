apiVersion: apps/v1
kind: Deployment
metadata:
  name: app-deploy
  namespace: app
spec:
  replicas: 2
  selector:
    matchLabels:
      app: app
  template:
    metadata:
      labels:
        app: app
    spec:
      containers:
        - name: app
          image: {{ IMAGE_NAME }}:{{ IMAGE_TAG }}
          ports:
            - containerPort: 5000
